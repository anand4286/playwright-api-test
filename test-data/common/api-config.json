{
  "apiEndpoints": {
    "baseUrl": "https://jsonplaceholder.typicode.com",
    "users": "/users",
    "posts": "/posts",
    "comments": "/comments"
  },
  "expectedStatusCodes": {
    "success": {
      "get": 200,
      "post": 201,
      "put": 200,
      "patch": 200,
      "delete": 200
    },
    "clientError": {
      "notFound": 404,
      "badRequest": 400,
      "unauthorized": 401,
      "forbidden": 403
    },
    "serverError": {
      "internalError": 500,
      "badGateway": 502,
      "serviceUnavailable": 503
    }
  },
  "responseStructure": {
    "user": ["id", "name", "email", "username"],
    "post": ["id", "title", "body", "userId"],
    "comment": ["id", "name", "email", "body", "postId"]
  },
  "timeouts": {
    "default": 30000,
    "long": 60000,
    "short": 5000
  }
}
